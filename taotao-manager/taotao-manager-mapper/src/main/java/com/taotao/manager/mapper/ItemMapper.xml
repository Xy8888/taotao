<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taotao.manager.mapper.ItemMapper">
    <!--addItem-->
    <insert id="addItem" parameterType="Item">
        INSERT  INTO tb_item(id,title,sell_point,price,num,barcode,image,cid,status,created,updated)
        VALUES(#{id},#{title},#{sellPoint},#{price},#{num},#{barcode},#{image},#{cid},#{status},#{created},#{updated})
    </insert>
</mapper>